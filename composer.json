{
  "name": "dolphiq/iconpicker",
  "description": "Craft Iconpicker field type that offers end-users an easy way to pick an icon/symbol from a .woff or .ttf font file.",
  "version": "2.0.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "icon",
    "symbol",
    "iconpicker"
  ],
  "type": "craft-plugin",
  "license": "MIT",
  "require": {
    "craftcms/cms": "^4.0.0",
    "phenx/php-font-lib": "^0.5.1"
  },
  "autoload": {
    "psr-4": {
      "plugins\\dolphiq\\iconpicker\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Dolphiq",
      "email": "info@dolphiq.nl",
      "homepage": "https://dolphiq.nl/"
    }
  ],
  "support": {
    "source": "https://github.com/Dolphiq/craft3-iconpicker",
    "docs": "https://github.com/Dolphiq/craft3-iconpicker/blob/master/README.md",
    "issues": "https://github.com/Dolphiq/craft3-iconpicker/issues"
  },
  "extra": {
    "handle": "dolphiq-iconpicker",
    "name": "Iconpicker"
  }
}
